{
  "name": "react-spotify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flow": "flow",
    "start": "./node_modules/.bin/vite",
    "build": "./node_modules/.bin/tsc && vite build",
    "serve": "./node_modules/.bin/vite preview",
    "lint:fix": "./node_modules/.bin/eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "./node_modules/.bin/prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,scss,css,md,json}\" ",
    "lint": "npm run lint:format && npm run lint:fix",
    "type-check": "./node_modules/.bin/tsc"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "antd": "^4.23.6",
    "bootstrap": "^5.2.2",
    "flow-bin": "^0.186.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.55.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-svgr": "^2.2.2",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pre-commit": "lint",
  "license": "MIT"
}
